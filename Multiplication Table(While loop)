#include <iostream>
#include <string>
using namespace std;
int main()
{
    int x;
    cout << "Enter the number to process its multiplication table (up to 10):" << endl;
    cin >> x;
    cout << "\n\nMultiplication table: " << endl;

    int y = 1;
    while (y <= 10) {
        cout << x << " * " << y << " = " << x * y << endl;
        y++;
    }
    return 0;
}
