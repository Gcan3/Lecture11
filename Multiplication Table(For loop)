#include <iostream>
#include <string>
using namespace std;
int main()
{
    int x;
    cout << "Enter the number to process its multiplication table (up to 10):" << endl;
    cin >> x;
    cout << "\n\nMultiplication table: " << endl;
    for (int y = 1; y <= 10; y++)
    {
        cout << x << " * " << y << " = " << x * y << endl;
    }

    return 0;
}
