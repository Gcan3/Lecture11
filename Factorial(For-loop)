#include <iostream>
#include <string>
using namespace std;
int main()
{
    int x, fact = 1;
    cout << "Enter the number to find its factorial:" << endl;
    cin >> x;
    for (int y = x; y > 0; y--)
    {
        fact = y * fact;
    }
    cout << "The factorial for " << x << " is: " << fact << endl;

    return 0;
}
