#include <iostream>
#include <string>
using namespace std;
int main()
{
	//Set values
	int x = 1, y = 1;
	int a = 1, b = 1;

	//Falling
	while (a <= 5)
	{
		b = a;
		while (b <= 5)
		{
			cout << "*";
			b++;
		}
		cout << endl;
		a++;
	}
	//Rising
	while (x <= 5)
	{
		y = 1;
		while (y <= x)
		{
			cout << "*";
			y++;
		}
		cout << endl;
		x++;
	}
	cin.get();
	return 0;
}
